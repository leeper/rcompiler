#!/bin/csh
setenv CMD "$*"
if (! ${?PRERUN}) setenv PRERUN ''
setenv CMD "${PRERUN} ${CMD}"
if (${PRERUN} != '') echo ${CMD}
setenv LD_LIBRARY_PATH @RCC_LIB_PATH@:@R_LIB_PATH@
setenv R_HOME @R_HOME@
setenv PATH .:$PATH
setenv RCC_R_INCLUDE_PATH @RCC_R_INCLUDE_PATH@
if (${PRERUN} == '') then
  eval $CMD
else
  exec $CMD
endif
